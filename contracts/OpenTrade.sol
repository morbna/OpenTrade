pragma solidity ^0.5.0;

import "./TradeCoin.sol";

contract OpenTrade {

  TradeCoin private _token;

  enum TradeStatus{ NEW, PENDING, COMPLETED, DELETED }

  struct Trade {
    TradeStatus status;
    uint price;
    }

  Trade [] public trades;

  mapping( uint => address) public tradeToSeller;
  mapping( uint => address) public tradeToBuyer;

  event NewListing(
    address indexed _from,
    uint indexed id,
    string indexed IndexedProductName, 
    string productName, 
    string productDescription, 
    string productPicture,
    uint productPrice);

  event DeleteListing(uint id);
  event TradeInit(uint id, address buyer);
  event TradeComplete(uint id);
  event TradeCancel(uint id);

  modifier onlyBuyer(address sender,uint tradeId) {
    require(sender == tradeToBuyer[tradeId], "Only buyer can call this method");
    _;
  }

  modifier onlySeller(address sender, uint tradeId) {
    require(sender == tradeToSeller[tradeId], "Only seller can call this method");
    _;
  }

  modifier onlyBuyerOrSeller(address sender, uint tradeId) {
    require(sender == tradeToSeller[tradeId] || sender == tradeToBuyer[tradeId] , "Only buyer or seller can call this method");
    _;
  }

  modifier notSeller(address sender, uint tradeId) {
    require(sender != tradeToSeller[tradeId], "Seller cannot call this method");
    _;
  }

  modifier isPending(uint tradeId) {
    require(trades[tradeId].status==TradeStatus.PENDING, "Trade is not pending");
    _;
  }
  
    modifier isNew(uint tradeId) {
    require(trades[tradeId].status==TradeStatus.NEW, "Trade is not new");
    _;
  }

  function createListing (
    string memory productName, 
    string memory productDescription,
    string memory productPicture,
    uint productPrice
    ) public returns(uint) {
      require(productPrice>0);
      trades.push(Trade(TradeStatus.NEW, productPrice));
      uint id = trades.length - 1;
      tradeToSeller[id] = msg.sender;
      tradeToBuyer[0] = address(0);
      emit NewListing(msg.sender, id, productName, productName, productDescription, productPicture,productPrice);
      return id;
  }

  // delete new listed produt
  function deleteListing(uint id) external onlySeller(msg.sender, id) isNew(id){
    trades[id].status = TradeStatus.DELETED;
    emit DeleteListing(id);
  }

  // purchase new listed product
  function purchaseListing(uint id) external payable notSeller(msg.sender, id) isNew(id) {
    _token.deposit(msg.sender, trades[id].price);
    tradeToBuyer[id] = msg.sender;
    trades[id].status = TradeStatus.PENDING;
    emit TradeInit(id, msg.sender);
  }

  // complete purchase
  function completeTrade(uint id) external onlyBuyer(msg.sender, id) isPending(id) {
    address payable seller = address(uint160(tradeToSeller[id]));
    _token.transferFunds(msg.sender, seller, trades[id].price);
    trades[id].status = TradeStatus.COMPLETED;
    emit TradeComplete(id);
  }

  // cancel in-progress purchase
  function cancelTrade(uint id) external onlyBuyerOrSeller(msg.sender, id) isPending(id){
    _token.withdraw(tradeToBuyer[id], trades[id].price);
    tradeToBuyer[id] = address(0);
    trades[id].status = TradeStatus.NEW;
    emit TradeCancel(id);
  }

  constructor (TradeCoin token) public {
    _token = token;

    createListing(
      'Sony Playstation 5', 
      'Sony PlayStation 5 Console - Disc Version PS5 Brand New',
      'data:image/jpeg;base64,',
      50
    );
    createListing(
      'XBOX Series X',
      'Brand new xbox series X in hand, unopened.',
      'data:image/jpeg;base64,',
      40
    );
    createListing(
      'Nintendo Switch Red/Blue V1.1',
      'This Nintendo Switch bundle includes a Mario Red & Blue Edition carrying case adorned with Super Mario iconography as well as a screen protector to keep your Nintendo Switch system secure.',
      'data:image/jpeg;base64,',
      30
    );
  }
}